<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://napkinlaf.sf.net/sketch/"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <title>Gold Medal Icon</title>
    <description>gold medal icon</description>
    <clippingBounds>
        <dimensions>
            <width>32</width>
            <height>32</height>
        </dimensions>
    </clippingBounds>
    <!-- grey medal shadow -->
    <templateItem>
        <drawStroke>false</drawStroke>
        <drawFill>true</drawFill>
        <fillColor>
            <r>128</r>
            <g>128</g>
            <b>128</b>
        </fillColor>
        <path>
            <moveTo>
                <point>
                    <x>29</x>
                    <y>17</y>
                </point>
            </moveTo>
            <cubicTo>
                <control>
                    <x>28</x>
                    <y>23</y>
                </control>
                <control>
                    <x>23</x>
                    <y>28</y>
                </control>
                <point>
                    <x>17</x>
                    <y>29</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>11</x>
                    <y>28</y>
                </control>
                <control>
                    <x>6</x>
                    <y>23</y>
                </control>
                <point>
                    <x>5</x>
                    <y>17</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>6</x>
                    <y>11</y>
                </control>
                <control>
                    <x>11</x>
                    <y>6</y>
                </control>
                <point>
                    <x>17</x>
                    <y>5</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>23</x>
                    <y>6</y>
                </control>
                <control>
                    <x>28</x>
                    <y>11</y>
                </control>
                <point>
                    <x>29</x>
                    <y>17</y>
                </point>
            </cubicTo>
        </path>
    </templateItem>
    <!-- bronze medal -->
    <templateItem>
        <drawStroke>true</drawStroke>
        <drawFill>true</drawFill>
        <strokeColor>
            <r>0</r>
            <g>0</g>
            <b>0</b>
        </strokeColor>
        <fillColor>
            <r>232</r>
            <g>52</g>
            <b>12</b>
        </fillColor>
        <path>
            <moveTo>
                <point>
                    <x>27</x>
                    <y>15</y>
                </point>
            </moveTo>
            <cubicTo>
                <control>
                    <x>26</x>
                    <y>21</y>
                </control>
                <control>
                    <x>21</x>
                    <y>26</y>
                </control>
                <point>
                    <x>15</x>
                    <y>27</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>9</x>
                    <y>26</y>
                </control>
                <control>
                    <x>4</x>
                    <y>21</y>
                </control>
                <point>
                    <x>3</x>
                    <y>15</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>4</x>
                    <y>9</y>
                </control>
                <control>
                    <x>9</x>
                    <y>4</y>
                </control>
                <point>
                    <x>15</x>
                    <y>3</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>21</x>
                    <y>4</y>
                </control>
                <control>
                    <x>26</x>
                    <y>9</y>
                </control>
                <point>
                    <x>27</x>
                    <y>15</y>
                </point>
            </cubicTo>
        </path>
    </templateItem>
    <!-- black star in centre of the medal -->
    <templateItem>
        <drawStroke>true</drawStroke>
        <drawFill>true</drawFill>
        <strokeColor>
            <r>0</r>
            <g>0</g>
            <b>0</b>
        </strokeColor>
        <fillColor>
            <r>255</r>
            <g>255</g>
            <b>0</b>
        </fillColor>
        <path>
            <moveTo>
                <point>
                    <x>15</x>
                    <y>5</y>
                </point>
            </moveTo>
            <lineTo>
                <point>
                    <x>17</x>
                    <y>13</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>25</x>
                    <y>15</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>17</x>
                    <y>17</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>15</x>
                    <y>25</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>13</x>
                    <y>17</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>5</x>
                    <y>15</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>13</x>
                    <y>13</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>15</x>
                    <y>5</y>
                </point>
            </lineTo>
        </path>
    </templateItem>
</template>
