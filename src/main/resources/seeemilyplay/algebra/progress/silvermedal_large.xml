<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://napkinlaf.sf.net/sketch/"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <title>Silver Medal Icon</title>
    <description>silver medal icon</description>
    <clippingBounds>
        <dimensions>
            <width>160</width>
            <height>160</height>
        </dimensions>
    </clippingBounds>
    <!-- grey medal shadow -->
    <templateItem>
        <drawStroke>false</drawStroke>
        <drawFill>true</drawFill>
        <fillColor>
            <r>128</r>
            <g>128</g>
            <b>128</b>
        </fillColor>
        <path>
            <moveTo>
                <point>
                    <x>145</x>
                    <y>85</y>
                </point>
            </moveTo>
            <cubicTo>
                <control>
                    <x>140</x>
                    <y>115</y>
                </control>
                <control>
                    <x>115</x>
                    <y>140</y>
                </control>
                <point>
                    <x>85</x>
                    <y>145</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>55</x>
                    <y>140</y>
                </control>
                <control>
                    <x>30</x>
                    <y>115</y>
                </control>
                <point>
                    <x>25</x>
                    <y>85</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>30</x>
                    <y>55</y>
                </control>
                <control>
                    <x>55</x>
                    <y>30</y>
                </control>
                <point>
                    <x>85</x>
                    <y>25</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>115</x>
                    <y>30</y>
                </control>
                <control>
                    <x>140</x>
                    <y>55</y>
                </control>
                <point>
                    <x>145</x>
                    <y>85</y>
                </point>
            </cubicTo>
        </path>
    </templateItem>
    <!-- silver medal -->
    <templateItem>
        <drawStroke>true</drawStroke>
        <drawFill>true</drawFill>
        <strokeColor>
            <r>0</r>
            <g>0</g>
            <b>0</b>
        </strokeColor>
        <fillColor>
            <r>192</r>
            <g>192</g>
            <b>225</b>
        </fillColor>
        <path>
            <moveTo>
                <point>
                    <x>135</x>
                    <y>75</y>
                </point>
            </moveTo>
            <cubicTo>
                <control>
                    <x>130</x>
                    <y>105</y>
                </control>
                <control>
                    <x>105</x>
                    <y>130</y>
                </control>
                <point>
                    <x>75</x>
                    <y>135</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>45</x>
                    <y>130</y>
                </control>
                <control>
                    <x>20</x>
                    <y>105</y>
                </control>
                <point>
                    <x>15</x>
                    <y>75</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>20</x>
                    <y>45</y>
                </control>
                <control>
                    <x>45</x>
                    <y>20</y>
                </control>
                <point>
                    <x>75</x>
                    <y>15</y>
                </point>
            </cubicTo>
            <cubicTo>
                <control>
                    <x>105</x>
                    <y>20</y>
                </control>
                <control>
                    <x>130</x>
                    <y>45</y>
                </control>
                <point>
                    <x>135</x>
                    <y>75</y>
                </point>
            </cubicTo>
        </path>
    </templateItem>
    <!-- black star in centre of the medal -->
    <templateItem>
        <drawStroke>true</drawStroke>
        <drawFill>true</drawFill>
        <strokeColor>
            <r>0</r>
            <g>0</g>
            <b>0</b>
        </strokeColor>
        <fillColor>
            <r>255</r>
            <g>255</g>
            <b>0</b>
        </fillColor>
        <path>
            <moveTo>
                <point>
                    <x>75</x>
                    <y>25</y>
                </point>
            </moveTo>
            <lineTo>
                <point>
                    <x>85</x>
                    <y>65</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>125</x>
                    <y>75</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>85</x>
                    <y>85</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>75</x>
                    <y>125</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>65</x>
                    <y>85</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>25</x>
                    <y>75</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>65</x>
                    <y>65</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>75</x>
                    <y>25</y>
                </point>
            </lineTo>
        </path>
    </templateItem>
</template>
