<?xml version="1.0" encoding="UTF-8"?>
<template xmlns="http://napkinlaf.sf.net/sketch/"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <title>Pointer NW</title>
    <description>a pointer that goes in a NW direction</description>
    <clippingBounds>
        <dimensions>
            <width>32</width>
            <height>32</height>
        </dimensions>
    </clippingBounds>
    
    <!-- the arrow body -->
    <templateItem>
      <drawStroke>true</drawStroke>
      <drawFill>true</drawFill>
      <strokeColor>
            <r>94</r>
            <g>7</g>
            <b>255</b>
      </strokeColor>
      <fillColor>
            <r>94</r>
            <g>7</g>
            <b>255</b>
      </fillColor>
      <path>
            <moveTo>
                <point>
                    <x>32</x>
                    <y>30</y>
                </point>
            </moveTo>
            <lineTo>
                <point>
                    <x>0</x>
                    <y>23</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>0</x>
                    <y>24</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>32</x>
                    <y>32</y>
                </point>
            </lineTo>
        </path>
    </templateItem>
    
    <!-- the bottom part of the arrow head -->
    <templateItem>
      <drawStroke>true</drawStroke>
      <drawFill>true</drawFill>
      <strokeColor>
            <r>94</r>
            <g>7</g>
            <b>255</b>
      </strokeColor>
      <fillColor>
            <r>94</r>
            <g>7</g>
            <b>255</b>
      </fillColor>
      <path>
            <moveTo>
                <point>
                    <x>10</x>
                    <y>32</y>
                </point>
            </moveTo>
            <lineTo>
                <point>
                    <x>0</x>
                    <y>24</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>0</x>
                    <y>23</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>11</x>
                    <y>32</y>
                </point>
            </lineTo>
        </path>
    </templateItem>
    
    <!-- the top part of the arrow head -->
    <templateItem>
      <drawStroke>true</drawStroke>
      <drawFill>true</drawFill>
      <strokeColor>
            <r>94</r>
            <g>7</g>
            <b>255</b>
      </strokeColor>
      <fillColor>
            <r>94</r>
            <g>7</g>
            <b>255</b>
      </fillColor>
      <path>
            <moveTo>
                <point>
                    <x>23</x>
                    <y>20</y>
                </point>
            </moveTo>
            <lineTo>
                <point>
                    <x>0</x>
                    <y>23</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>0</x>
                    <y>24</y>
                </point>
            </lineTo>
            <lineTo>
                <point>
                    <x>24</x>
                    <y>20</y>
                </point>
            </lineTo>
        </path>
    </templateItem>
   
</template>